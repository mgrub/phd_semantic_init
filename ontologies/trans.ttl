@prefix xml:    <http://www.w3.org/XML/1998/namespace> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix om:     <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix sosa:   <http://www.w3.org/ns/sosa/> .
@prefix ssn:    <http://www.w3.org/ns/ssn/> .
@prefix scal:   <http://www.example.com/ns/scal/> .
@prefix si:     <https://ptb.de/si#> .

@prefix math: <http://cll.niimm.ksu.ru/ontologies/mathematics#> .
@prefix trans:     <http://www.example.com/ns/trans/> .


trans: rdf:type owl:Ontology ;
  rdfs:label "Sensor Dynamic Transfer Behavior Ontology (trans)"@en .



# main concepts

trans:TransferModel rdf:type owl:Class ;
  rdfs:subClassOf 
    scal:CalibrationModel ,
    [ rdf:type owl:Restriction ;
      owl:onProperty trans:isExpressedBy ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass trans:MathematicalObject
    ] .

trans:MathematicalObject rdf:type owl:Class ;
  rdfs:subClassOf math:E34 .

trans:AnalyticalDomain rdf:type owl:Class ;
  rdfs:comment "The domain in which the transfer behavior is represented/analyzed"@en .

trans:SystemType rdf:type owl:Class .

trans:QualitativeBehavior rdf:type owl:Class .



# main relations

trans:isExpressedBy rdf:type owl:ObjectProperty ;
  schema:domainIncludes trans:TransferModel ;
  schema:rangeIncludes trans:MathematicalObject .

trans:isDefinedInDomain rdf:type owl:ObjectProperty ;
  schema:domainIncludes trans:TransferModel ;
  schema:rangeIncludes trans:AnalyticDomain .

trans:isOfSystemType rdf:type owl:ObjectProperty ;
  schema:domainIncludes trans:TransferModel ;
  schema:rangeIncludes trans:SystemType .

trans:hasBehavior rdf:type owl:ObjectProperty ;
  schema:domainIncludes trans:TransferModel ;
  schema:rangeIncludes trans:QualitativeBehavior .

trans:hasValueStructure rdf:type owl:ObjectProperty ,
  owl:FunctionalProperty ;
  schema:domainIncludes trans:TransferModel ;
  schema:rangeIncludes trans:DimensionStructure .

trans:hasFrequencyStructure rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasValueStructure .

trans:hasTimeStructure rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasValueStructure .



# classes derived from TransferModel

trans:FilterType rdf:type owl:Class ;
  rdfs:subClassOf trans:TransferModel ;
  trans:isDefinedInDomain trans:FrequencyDomain ;
  trans:hasBehavior trans:FrequencyBehavior .

trans:Chebyshev rdf:type owl:Class ;
  rdfs:subClassOf trans:FilterType ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isExpressedBy ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Polynomial
  ] .

trans:Butterworth rdf:type owl:Class ;
  rdfs:subClassOf trans:FilterType .

trans:Bessel rdf:type owl:Class ;
  rdfs:subClassOf trans:FilterType ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isExpressedBy ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Polynomial
  ] .

trans:Elliptic rdf:type owl:Class ;
  rdfs:subClassOf trans:FilterType ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isExpressedBy ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:EllipticRationalFunction
  ] .


trans:StepResponseModel rdf:type owl:Class ;
  rdfs:subClassOf trans:TransferModel ;
  trans:hasBehavior trans:Dynamic ;
  trans:isDefinedInDomain trans:TimeDomain ;
  trans:isOfSystemType trans:LinearSystem .

trans:ImpulseResponseModel rdf:type owl:Class ;
  rdfs:subClassOf trans:TransferModel ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isDefinedInDomain ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass [ rdf:type owl:Class ;
      owl:unionOf ( trans:FrequencyDomain trans:TimeDomain )
    ]
  ] ;
  trans:hasBehavior trans:Dynamic .

trans:ContinuousImpulseResponseModel rdf:type owl:Class ;
  rdfs:subClassOf trans:ImpulseResponseModel ;
  trans:isOfSystemType trans:LinearSystem .

trans:InfiniteImpulseResponseModel rdf:type owl:Class ;
  rdfs:subClassOf trans:ImpulseResponseModel ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isExpressedBy ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass [ rdf:type owl:Class ;
      owl:unionOf ( trans:LinearDifferenceEquation trans:RationalFraction )
    ]
  ] .

trans:FiniteImpulseResponseModel rdf:type owl:Class ;
  rdfs:subClassOf trans:ImpulseResponseModel ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isExpressedBy ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass [ rdf:type owl:Class ;
      owl:unionOf ( trans:LinearDifferenceEquation trans:Polynomial )
    ]
  ] .


trans:LinearAffineModel rdf:type owl:Class ;
  rdfs:subClassOf trans:TransferModel , 
    [ rdf:type owl:Restriction ;
      owl:onProperty trans:isExpressedBy ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass trans:GainOffsetNotation
    ];
  trans:isDefinedInDomain trans:TimeDomain ;
  trans:hasBehavior trans:Static ;
  trans:isOfSystemType trans:NonlinearSystem .


trans:StateSpaceModel rdf:type owl:Class ;
  rdfs:subClassOf trans:TransferModel ;
  trans:isDefinedInDomain trans:TimeDomain ;
  trans:hasBehavior trans:Dynamic .

trans:LinearStateSpaceModel rdf:type owl:Class ;
  rdfs:subClassOf trans:StateSpaceModel ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:isExpressedBy ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:StateSpaceMatrixNotation
  ] .

trans:NonLinearStateSpaceModel rdf:type owl:Class ;
  rdfs:subClassOf trans:StateSpaceModel ;
  trans:isOfSystemType trans:NonlinearSystem .



# classes derived from MathematicalObject

trans:Array rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  om:Measure ,
  [ rdf:type owl:Restriction ;
    owl:onProperty om:hasNumericalValue ;
    owl:allValuesFrom [ rdf:type rdfs:Datatype ;
      owl:unionOf ( 
        trans:MathMLLiteral
        trans:TeXLiteral
        rdf:XMLLiteral
      )
    ]
  ] ;
  rdfs:seeAlso math:E1687 .

trans:ArrayWithUncertainty rdf:type owl:Class ;
  rdfs:subClassOf si:MeasureWithUncertainty ,
  trans:Array .

trans:FrequencySpectrum rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasFrequencyArray ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasValueArray ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] .

trans:LinearDifferenceEquation rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasCoefficients ;
    owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] .

trans:LinearOrdinaryDifferentialEquation rdf:type owl:Class ;
  owl:equivalentClass math:E2822 ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasCoefficients ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] .

trans:Polynomial rdf:type owl:Class ;
  owl:equivalentClass math:E1995 ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasCoefficients ;
    owl:allValuesFrom trans:Array
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasPolynomialRoots ;
    owl:onClass trans:Array
  ] ;
  rdfs:comment "An expression consisting of variables (also called indeterminates) and coefficients, that involves only the operations of addition, subtraction, multiplication, and non-negative integer exponentiation of variables"@en .

trans:RationalFraction rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasDenominator ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Polynomial
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasNumerator ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Polynomial
  ] ;
  rdfs:comment "A rational fraction given by its numerator- and denominator-polynomial."@en ;
  rdfs:seeAlso math:E1603 .

trans:EllipticRationalFunction rdf:type owl:Class ;
  rdfs:subClassOf trans:RationalFraction .

trans:StateSpaceMatrixNotation rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasFeedthroughMatrix ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasInputMatrix ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasOutputMatrix ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasSystemMatrix ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] ;
  rdfs:comment """Depending on chosen time-dimension structure (continuous / discrete) the matrices represent a system of differential or difference equations. 

continuous:
x_dot = A*x + B*u
y = C*x + D*u

discrete:
x[n+1] = A*x[n] + B*u[n]
y[n] = C*x[n] + D*u[n]

with internal state x, state's time derivate x_dot, input u, output y."""@en ,
  "State Space Model represented by its system Matrix A, input matrix B, output matrix C and feedthrough matrix D."@en ;
  rdfs:seeAlso math:E2823 .

trans:TimeSeries rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasValueArray ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasTimeArray ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass trans:Array
  ] .

trans:GainOffsetNotation rdf:type owl:Class ;
  rdfs:subClassOf trans:MathematicalObject ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasGain ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass om:Measure
  ] ,
  [ rdf:type owl:Restriction ;
    owl:onProperty trans:hasOffset ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass om:Measure
  ] ;
  rdfs:comment "Linear affine model: y = gain * x + offset" .

# relations for MathematicalObject

trans:hasArrayDefinition rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf om:hasValue ;
  schema:domainIncludes trans:MathematicalObject ;
  schema:rangeIncludes trans:Array .

trans:hasGain rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf om:hasValue ;
  schema:domainIncludes trans:MathematicalObject ;
  schema:rangeIncludes om:Measure .

trans:hasOffset rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf om:hasValue ;
  schema:domainIncludes trans:MathematicalObject ;
  schema:rangeIncludes om:Measure .

trans:hasCoordinates rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition .

trans:hasTimeArray rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasCoordinates ;
  schema:domainIncludes trans:TimeSeries .

trans:hasFrequencyArray rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasCoordinates ;
  schema:domainIncludes trans:FrequencySpectrum .

trans:hasValueArray rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition .

trans:hasCoefficients rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition ;
  rdfs:comment "A polynomial is defined by its coefficients stored in an array." .

trans:hasPolynomialRoots rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition ;
  schema:domainIncludes trans:Polynomial ;
  rdfs:comment "A polynomial is defined by its roots stored in an array." .

trans:hasNumerator rdf:type owl:ObjectProperty ;
  schema:domainIncludes trans:RationalFraction ;
  schema:rangeIncludes trans:Polynomial .

trans:hasDenominator rdf:type owl:ObjectProperty ;
  schema:domainIncludes trans:RationalFraction ;
  schema:rangeIncludes trans:Polynomial .

trans:hasSystemMatrix rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition ;
  schema:domainIncludes trans:StateSpaceMatrixNotation ;
  schema:rangeIncludes trans:Array .

trans:hasInputMatrix rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition ;
  schema:domainIncludes trans:StateSpaceMatrixNotation ;
  schema:rangeIncludes trans:Array .

trans:hasFeedthroughMatrix rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition ;
  schema:domainIncludes trans:StateSpaceMatrixNotation ;
  schema:rangeIncludes trans:Array .

trans:hasOutputMatrix rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf trans:hasArrayDefinition ;
  schema:domainIncludes trans:StateSpaceMatrixNotation ;
  schema:rangeIncludes trans:Array .



# additional properties of TransferModel

trans:FrequencyBehavior rdf:type owl:Class ;
  rdfs:subClassOf trans:QualitativeBehavior .

trans:LowPass rdf:type owl:Class ;
  rdfs:subClassOf trans:FrequencyBehavior .

trans:HighPass rdf:type owl:Class ;
  rdfs:subClassOf trans:FrequencyBehavior .

trans:BandPass rdf:type owl:Class ;
  rdfs:subClassOf trans:FrequencyBehavior .

trans:BandStop rdf:type owl:Class ;
  rdfs:subClassOf trans:FrequencyBehavior .


trans:TemporalBehavior rdf:type owl:Class ;
  rdfs:subClassOf trans:QualitativeBehavior .

trans:Dynamic rdf:type owl:Class ;
  rdfs:subClassOf trans:TemporalBehavior ;
  rdfs:comment "System response can depend on the history of input values."@en .

trans:Static rdf:type owl:Class ;
  rdfs:subClassOf trans:TemporalBehavior ;
  rdfs:comment "System response is only dependent on the last input value."@en .

trans:LinearSystem rdf:type owl:Class ;
  rdfs:subClassOf trans:SystemType ;
  owl:disjointWith trans:NonlinearSystem .

trans:LTISystem rdf:type owl:Class ;
  rdfs:subClassOf trans:LinearSystem .

trans:NonlinearSystem rdf:type owl:Class ;
  rdfs:subClassOf trans:SystemType .


trans:FrequencyDomain rdf:type owl:Class ;
  rdfs:subClassOf trans:AnalyticalDomain .

trans:TimeDomain rdf:type owl:Class ;
  rdfs:subClassOf trans:AnalyticalDomain .

trans:DimensionStructure rdf:type owl:Class .

trans:Continuous rdf:type owl:Class ;
  rdfs:subClassOf trans:DimensionStructure ;
  owl:disjointWith trans:Discrete ;
  rdfs:comment "The values along a dimension are represented by continuous points."@en .

trans:Discrete rdf:type owl:Class ;
  rdfs:subClassOf trans:DimensionStructure ;
  rdfs:comment "The values along a dimension are represented by discrete points."@en .



# Datatypes

trans:MathMLLiteral rdf:type rdfs:Datatype ;
  owl:equivalentClass rdf:XMLLiteral .

trans:TeXLiteral rdf:type rdfs:Datatype ;
  owl:equivalentClass xsd:string .



# General axioms

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( trans:BandPass
  trans:BandStop
  trans:HighPass
  trans:LowPass
  )
] .

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( trans:Bessel
  trans:Butterworth
  trans:Chebyshev
  trans:Elliptic
  )
] .
