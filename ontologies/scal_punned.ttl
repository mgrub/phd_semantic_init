  a ontoclean:NonDependentClass .

geo:SpatialObject a owl:NamedIndividual ;
  a ontoclean:Class ;
  a ontoclean:RigidClass ;
  a ontoclean:AntiUnityClass ;
  a ontoclean:SortalClass ;
  a ontoclean:NonDependentClass .

om:Measure a owl:NamedIndividual ;
  a ontoclean:Class ;
